<?xml version="1.0"?>
<include>
  <configuration name="acl.conf" description="Network Lists">
    <network-lists>
	{% for bl  in netlist %}
		<list name="{{ bl.get_name() }}" default="{{ bl.default_type }}">
			{% for iteam  in bl.items %}
			<node bluebox="net_list_item_{{ iteam.id }}" type="{{ iteam.type }}" cidr="{{ iteam.record }}"/>
			{% endfor %}
		</list>
	{% endfor %}
	
